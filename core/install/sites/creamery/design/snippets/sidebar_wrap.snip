<!-- sidebar_wrap begin -->
<div id="sidebar-wrapper">
   <div id="sidebar">
      <et:if_content part="sidebar">
         <et:content part="sidebar" inherit="true" />
       <et:else />
         <et:if_content part="sidebar_category" inherit="true">
            <et:design:snippet name="sidebar">
               <et:content part="sidebar_category" inherit="true" />
            </et:design:snippet>
         </et:if_content>
      </et:if_content>
   </div>
</div>
<!-- sidebar_wrap end -->