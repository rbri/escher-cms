<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:dc="http://purl.org/dc/elements/1.1/">
   <channel>
      <title>Article RSS Feed</title>
      <link><et:site_name /></link>
      <language>en-us</language>
      <ttl>40</ttl>
      <description>The main blog feed for my Web site.</description>
      <et:find url="/news">
         <et:children:each limit="10" order="desc">
            <item>
               <title><et:title /></title>
               <description><et:escape_html><et:content /></et:escape_html></description>
               <pubDate><et:date format="rss" /></pubDate>
               <guid><et:url full="true" /></guid>
               <link><et:url full="true" /></link>
            </item>
         </et:children:each>
      </et:find>
   </channel>
</rss>